<% layout("/layouts/boilerplate.ejs") %>
<body>
    <h3>All Listings</h3>
    <form methods="GET" action="/listings/new">
        <button>create new listings</button>
    </form>
    <ul>
        <% for(let listing of allListings) { %> 
            <li><a href="/listings/<%= listing._id %>"><%= listing.title %></a></li>
        <% } %>  
    </ul>
</body>
 